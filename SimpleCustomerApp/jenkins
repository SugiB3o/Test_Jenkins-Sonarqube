 pipeline {
        agent none
        stages {
          stage("build & SonarQube analysis") {
            agent any
            steps {
              withSonarQubeEnv('Sonarqube8.0') {
                sh "${scannerHome}/bin/sonar-scanner -Dsonar.java.binaries=**/*"
              }
            }
          }
          stage("Quality Gate") {
            steps {
              timeout(time: 3, unit: 'MINUTES') {
                waitForQualityGate abortPipeline: true
              }
            }
          }
        }
      }
